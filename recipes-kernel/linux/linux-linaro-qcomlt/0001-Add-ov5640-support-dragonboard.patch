+static struct i2c_driver ov5640_i2c_driver = {
+	.driver = {
+		.of_match_table = of_match_ptr(ov5640_of_match),
+		.name  = "ov5640",
+	},
+	.probe  = ov5640_probe,
+	.remove = ov5640_remove,
+	.id_table = ov5640_id,
+};
+
+module_i2c_driver(ov5640_i2c_driver);
