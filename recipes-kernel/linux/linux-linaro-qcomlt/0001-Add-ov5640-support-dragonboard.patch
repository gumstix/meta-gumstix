+static int ov5640_set_awb(struct ov5640 *ov5640, s32 enable_auto)
+{
+	u8 val = 0;
+
+	if (!enable_auto)
+		val = OV5640_AWB_MANUAL_ENABLE;
+
+	return ov5640_write_reg(ov5640, OV5640_AWB_MANUAL_CONTROL, val);
+}
